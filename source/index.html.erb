---
title: Multunus | Goodbye
---
<section class="main-header">
    <img src="/images/home/home-multunus-logo.svg" class="brand-logo">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <img class="bye-emoji" src="/images/home/bye.svg">
                <h1><%= data.home.title %></h1>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-10 col-lg-offset-1 col-md-10 col-md-offset-1 col-sm-12">
                <% data.home.description.each do |description| %>
                <p class="lead-main"><%= description.para %></p>
                <% end %>
            </div>
        </div>
    </div>
</section>

<!-- Our team Section -->
<section id="team" class="team content-section">
    <div class="container">
        <div class="row">
            <div class="role-filter-pills">
                <ul class="nav nav-pills">
                    <li class="nav-item active">
                        <a class="nav-link" data-toggle="portfilter" data-target="all">All</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-toggle="portfilter" data-target="pointy-haired">Pointy</i> Haired</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-toggle="portfilter" data-target="technology">Technology</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-toggle="portfilter" data-target="product">Product</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-toggle="portfilter" data-target="people-culture">People & Culture</a>
                    </li>
                </ul>
            </div>
        </div>
        <div class="row text-center">
            <div class="container">
                <div class="row">
                    <% data.home.team.each do |member| %>
                        <div class="col-md-4 col-sm-6 col-xs-12" data-tag="<%= member.tag %>">
                            <div class="team-member">
                                <figure>
                                    <%= image_tag member.image, class: "img-responsive" %>
                                    <figcaption>
                                        <p><%= member.description %></p>
                                        <ul>
                                            <li><a href=""><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
                                            <li><a href=""><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
                                            <li><a href=""><i class="fa fa-linkedin" aria-hidden="true"></i></a></li>
                                        </ul>
                                    </figcaption>
                                </figure>
                                <div class="member-brief">
                                    <h4 class="name"><%= member.name %></h4>
                                    <p class="role"><%= member.role %></p>
                                </div>
                            </div>
                        </div>
                    <% end %>
                </div>
            </div>
        </div>
    </div>
</section>