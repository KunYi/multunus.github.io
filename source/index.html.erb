---
title: Multunus | Goodbye
---
<section class="main-header">
  <img src="/images/home-multunus-logo.svg" class="brand-logo">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <h1><%= data.home.title %></h1>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-8 offset-lg-2 col-md-10 offset-md-1">
        <p class="lead">
          <%= data.home.description %>
        </p>
      </div>
    </div>
  </div>
</section>
<section class="our-team">
  <div class="container">
    <div class="row">
      <div class="role-filter-pills">
        <ul class="nav nav-pills">
          <li class="nav-item">
            <a class="nav-link active" href="#">All</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">Pointy Haired</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">Technology</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">Product</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">People & Culture</a>
          </li>
        </ul>
      </div>
    </div>
    <div class="row">
      <% data.home.team.each do |member| %>
        <div class="col-lg-3 col-md-6">
          <div class="profile-pic-wrapper">
            <%= image_tag member.image %>
            <div class="profile-pic-overlay">
              <div class="profile-name"><%= member.name %></div>  
              <div class="profile-job-title"><%= member.role %></div>          
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</section>